DEFINE FILE reporting/bv_support_cases
SUPPORT_TEAM/A50V=IF OWNER_MANAGER IS MISSING THEN 'OTHER' ELSE 'IBI';
END
 
TABLE FILE reporting/bv_support_cases
 
BY OWNER__USER_
BY OWNER_MANAGER
BY SUPPORT_TEAM
WHERE OWNER__USER_ NE MISSING
ON TABLE HOLD AS extracts/support_names_focus FORMAT XLSX
END
